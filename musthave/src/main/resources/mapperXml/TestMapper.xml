<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.TestMapper"><!--인터페이스 위치 -->

    <insert id="insertTest" parameterType="mybatis.dto.TestDTO">
        insert
        into test(name, age)
        values(#{name}, #{age})
    </insert>

    <select id="selectAll" resultType="mybatis.dto.TestDTO">
        select *
        from test
    </select>
</mapper>